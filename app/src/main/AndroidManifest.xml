<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.finaliproject">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Class Register"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FINALIPROJECT">
        <activity android:name=".StudentDetails"></activity>
        <activity
            android:name=".ClassList"
            android:parentActivityName=".StudentList" />
        <activity
            android:name=".ClassRegister"
            android:parentActivityName=".StudentList" />
        <activity android:name=".Welcome" />
        <activity
            android:name=".MainActivity2"
            android:parentActivityName="" />
        <activity android:name=".Login2" />
        <activity android:name=".Register" />
        <activity
            android:name=".AboutApp"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name=".StudentList"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>